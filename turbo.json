{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "$NODE_ENV",
        "$NEXT_PUBLIC_GA",
        "$NEXT_PUBLIC_MSC",
        "$NEXT_PUBLIC_SANITY_PROJECT_ID",
        "$NEXT_PUBLIC_SANITY_DATASET",
        "$NEXT_PUBLIC_SANITY_API_VERSION"
      ],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
